.container {
  width: 750rpx;
  margin-bottom: 30rpx;
  box-shadow: inset 0 1rpx 0 0 #e5e5e5, inset 0 -1rpx 0 0 #e5e5e5;
  background-color: #fff;
}

.header {
  font-size: 34rpx;
  font-weight: bold;
  color: #2d3034;
  line-height: 108rpx;
  padding-left: 30rpx;
  background-color: #fff;
}

.content {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}

.image {
  width: 750rpx;
}
